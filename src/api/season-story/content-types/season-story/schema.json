{
  "kind": "collectionType",
  "collectionName": "season_stories",
  "info": {
    "singularName": "season-story",
    "pluralName": "season-stories",
    "displayName": "Season Story"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "season": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::season.season",
      "inversedBy": "season_stories"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "text": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  }
}
